@page "/administrator/user/groups/{GroupId}/edit"
<MudPaper Class="ma-6" Elevation="0">
    <MudText Typo="Typo.h5">Изменить группу пользователей</MudText>
</MudPaper>
<MudDivider DividerType="DividerType.FullWidth" />
<MudPaper Class="ma-6" Elevation="0">
    <MudButton OnClick="()=>UpdateRole()" Variant="Variant.Filled" StartIcon="@Icons.Material.Filled.Save" Color="Color.Success" Size="Size.Medium">Сохранить</MudButton>
    <MudButton Class="mx-2" OnClick="()=>Cancel()" Variant="Variant.Filled" StartIcon="@Icons.Material.Filled.Cancel" Color="Color.Error" Size="Size.Medium">Отмена</MudButton>
</MudPaper>