<MudPaper Elevation="0" Class="mud-width-full pa-3 ma-0">
    <MudText Typo="Typo.body2" Class="pt-0" Color="Color.Dark"><b>Достижения</b></MudText>
    <MudPaper Class="d-flex mud-width-full pa-2" Elevation="0">
        <MudGrid>
            <MudItem xs="1">
                <MudIcon Icon="fas fa-fish" Color="Color.Warning" Size="Size.Medium" Title="Рыба: Специалист 2 уровня"></MudIcon>
            </MudItem>
            <MudItem xs="10">
                <MudProgressLinear Color="Color.Warning" Value="2" Min="0" Max="50" Style="margin-top:10px; margin-left: 20px;" />
            </MudItem>
            <MudItem xs="1">
                <MudIcon Icon="fas fa-pizza-slice" Color="Color.Error" Size="Size.Medium" Title="Пицца: Мастер 3 уровня"></MudIcon>
            </MudItem>
            <MudItem xs="10">
                <MudProgressLinear Color="Color.Error" Value="3" Min="0" Max="10" Style="margin-top:10px; margin-left: 20px;" />
            </MudItem>
            <MudItem xs="1">
                <MudIcon Icon="fas fa-bread-slice" Color="Color.Info" Size="Size.Medium" Title="Выпечка: Профессионал 4 уровня"></MudIcon>
            </MudItem>
            <MudItem xs="10">
                <MudProgressLinear Color="Color.Info" Value="4" Min="0" Max="10" Style="margin-top:10px; margin-left: 20px;" />
            </MudItem>
            <MudItem xs="1">
                <MudIcon Icon="fas fa-apple-alt" Color="Color.Tertiary" Size="Size.Medium" Title="Рецепты из яблок: Ученик 9 уровня"></MudIcon>
            </MudItem>
            <MudItem xs="10">
                <MudProgressLinear Color="Color.Tertiary" Value="9" Min="0" Max="10" Style="margin-top:10px; margin-left: 20px;" />
            </MudItem>
            <MudItem xs="1">
                <MudIcon Icon="fas fa-ice-cream" Color="Color.Dark" Size="Size.Medium" Title="Мороженое: Новичок 3 уровня"></MudIcon>
            </MudItem>
            <MudItem xs="10">
                <MudProgressLinear Color="Color.Dark" Value="3" Min="0" Max="10" Style="margin-top:10px; margin-left: 20px;" />
            </MudItem>
        </MudGrid>
    </MudPaper>
</MudPaper>
